.iconed-link {
  text-decoration: none;
  display: inline-flex;
  align-items: center;
  color: blue;
  font-size: 18px;
  font-weight: bold;
  @include phones {
    align-items: left;
    font-size: 21px;
    padding: 0;
    margin: 0;
    }
 
  &:before {
    content: "";
    display: inline-block;
    width: 20px;
    height: 20px;
    background: svg-load('link.svg', fill=blue) center center no-repeat;
    margin-right: 20px;
  }
}